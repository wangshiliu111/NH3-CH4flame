FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType   RAS;

RAS
{
    RASModel        kEpsilon;

    turbulence      on;
    printCoeffs     on;

    C1              1.44;
    C2              1.92;

    sigmak          1;
    sigmaEps        1.3;
}

