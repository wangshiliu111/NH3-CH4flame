FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0.44";
    object      AR;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    fixedWall
    {
        type            zeroGradient;
    }
    pilot
    {
        type            fixedValue;
        value           uniform 0;
    }
    airBC
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    wall
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    Axis
    {
        type            symmetryPlane;
    }
    frontAndBack
    {
        type            empty;
    }
    frontAndBack_pos
    {
        type            wedge;
    }
    frontAndBack_neg
    {
        type            wedge;
    }
}


